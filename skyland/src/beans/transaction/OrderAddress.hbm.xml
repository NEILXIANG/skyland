<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
      "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
          "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="beans.transaction">
	<class name="OrderAddress"  table="transaction_order_address" dynamic-insert="true" dynamic-update="true" >
		<id name="num" column="num" type="int" >
			<generator class="increment" />
		</id>
		<property name="userNum" column="user_num" type="int"/>
		<property name="useable" column="useable" type="boolean"/>
		<property name="name" column="name_" type="string"/>
		<property name="address" column="address" type="string"/>
		<property name="postcode" column="postcode" type="string"/>
		<property name="areaCode" column="area_code" type="string"/>
		<property name="telephone" column="telephone" type="string"/>
		<property name="mobilphone" column="mobilphone" type="string"/>

	</class>

</hibernate-mapping>


<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
      "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
          "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="beans.user.GoodsCart"  table="user_goods_cart" dynamic-insert="true" dynamic-update="true" >
		<id name="num" column="num" type="int" >
			<generator class="increment" />
		</id>
		<property name="userNum" column="user_num" type="int"/>
		<property name="time" column="time_" type="timestamp"/>
		<property name="useable" column="useable" type="boolean"/>
		<property name="goodsNum" column="goods_num" type="int"/>
		<property name="goodsCount" column="goods_count" type="int"/>
	</class>

</hibernate-mapping>

